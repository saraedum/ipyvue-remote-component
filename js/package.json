{
  "name": "ipyvue-remote-component",
  "version": "1.0.0",
  "description": "Load Vue.js Packages at Runtime in Jupyter Notebooks and JupyterLab",
  "author": "Julian RÃ¼th",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saraedum/ipyvue-remote-component.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../ipyvue_remote_component/labextension/ && rimraf ../ipyvue_remote_component/nbextension",
    "prepublish": "yarn run clean && yarn run build:prod",
    "build": "webpack --mode=development && yarn run build:labextension:dev",
    "build:prod": "webpack --mode=production && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "rimraf": "^2.6.1",
    "webpack": "^5"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "jupyter-vue": "^1.5.0",
    "lodash": "^4.17.4",
    "vue-remote-component": "../../vue-remote-component/vue-remote-component"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../ipyvue_remote_component/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "jupyter-vue": {
        "bundled": false
      },
      "vue": {
        "bundled": false
      }
    }
  }
}
